package Day16;

public class Frequency {

	void freq(int[] a)
	{
		
		for(int i=0;i<a.length;i++)
		{int count=0;
			
		for(int j =i; j<a.length;j++)
		{
			if(a[i]==a[j])
			{
				count++;
			}
		}
		System.out.println(a[i]+"---"+count);
		}
		
	}
	public static void main(String[] args) {
		Frequency freq = new Frequency();
		int[] a = {1,2,2,3,1,5,2};
		freq.freq(a);
	}

}
