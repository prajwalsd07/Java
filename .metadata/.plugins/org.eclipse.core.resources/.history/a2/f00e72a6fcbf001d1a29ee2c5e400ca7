package Day21;
class car extends Thread
{
	public car() throws InterruptedException {
		System.out.println("hi");
		Thread.sleep(1000);
	}
}

public class ThreadExample {

	public static void main(String[] args) throws InterruptedException {

		car c = new car();
		c.start();
		
		System.out.println("main");
		c.join();
		

	}

}
