package Day19;

class A {
	int show(int n) {
		int a = n + 10;
		return a;
	}

	String show2(String s) {
		String a = s + "this is class A";
		return a;
	}
}

class B extends A {
	@Override
	int show(int n) {

		return super.show(n);
	}

	@Override
	String show2(String s) {

		return super.show2(s);
	}

	boolean dark() {
		return false;
	}
}

public class InheritenceExample {

	public static void main(String[] args) {

	}

}
