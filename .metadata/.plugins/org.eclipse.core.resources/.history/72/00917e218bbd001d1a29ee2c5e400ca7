package Day18;

class car {
	static int speed = 80;
	static int size = 6;
	int cost;

	static String getdetails(int speed) {
		if (speed > 50) {
			return "toofast";
		}
		System.out.println(size);
		return "safe";

	}
}

public class SaticExapmle {

	public static void main(String[] args) {
		System.out.println(car.getdetails(90));
	}

}
