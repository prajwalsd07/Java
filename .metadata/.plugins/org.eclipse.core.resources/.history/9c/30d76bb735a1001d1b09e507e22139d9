package Day6;

public class MergeSort {
	
	
	public void mergeSort(int[] arr,int l,int h)
	{
		if(l<h)
		{
			int mid = (l+h)/2;
			mergeSort(arr,l,mid);
			mergeSort(arr,mid+1,h);
			merge(arr,l,mid,h);
		}
	}

